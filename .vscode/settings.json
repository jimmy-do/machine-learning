{
    "workbench.colorTheme": "Everforest Dark",
    "workbench.colorCustomizations": {
      "editorBracketHighlight.foreground1": "#BBBBBB",
      "editorBracketHighlight.foreground2": "#BBBBBB",
      "editorBracketHighlight.foreground3": "#BBBBBB",
      // "editor.foreground": "#d5b88c", //d5b88c,
    },
    "editor.fontFamily": "Hack, 'Courier New', monospace",
    "editor.fontSize": 13,
    // "editor.fontWeight": 300,
    "window.zoomLevel": 1.8,
    "editor.lineHeight": 22,
    "editor.letterSpacing": -0.6,
  
    // "editor.semanticTokenColorCustomizations": {
    //   "[Correia Github+EverForest Theme]": {
    //     "rules": {
    //       "parameter": {
    //         "fontStyle": "italic strikethrough",
    //         "foreground": "#dedad7"
    //       }
    //     }
    //   }
    // },
  
    "editor.tokenColorCustomizations": {
      "[Everforest Dark]": {
        "textMateRules": [
          {
            "scope": "support.function.builtin.python",
            "settings": {
              // "foreground": ""
            }
          },
          {
            "scope": "entity.name.function",
            "settings": {
              // "foreground": "#D5B88C"
            }
          },
          {
            // "scope": ["keyword.control","keyword.operator.logical.python","keyword.control.flow.python"],
            "scope": ["meta.attribute.python"],
            "settings": { "foreground": "#c3b9c6f6" } //d2b6daf6, bbb3bdf6, c3b9c6f6
          },
        ]
      }
    }
  }
  
